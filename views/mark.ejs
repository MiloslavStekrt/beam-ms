<%- include("header", {
  title: "MBS - My mark",
  stylefile: "mark",
  role: role,
  role: role,
}) %>
<main>
  <section class="sidebar">
    <h1>Classes</h1>
    <% lessons.map(lesson => { %>
      <p><a href=""><%= lesson.name %></a></p>
    <% }) %>
  </section>
  <section class="statistic">
    <article class="student">
      <h1>Subject</h1><h1>AVG: Zero</h1>
    </article>
    <article class="exams">
      <span class="first-span">
        <p>Name of exam</p>
        <p>mark</p>
      </span/>
      <% if(typeof selected_lesson !== "undefined"){ %>
        <span class="first-span">
          <p>Name of exam</p>
          <p>mark</p>
        </span/>
      <% } else { %>
        <h1>Lesson is not selected</h1>
      <% } %>
    </article>
  </section>
</main>
</html>
