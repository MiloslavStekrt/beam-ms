<%- include("header", {
  title: "MSB - creating new exam ",
  stylefile: "new_exam",
  role: role,
}) %>
<form action="/teacher/exams/create" method="post">
  <input type="text" name="title" placeholder="Name your exam">
  <input type="text" name="question" placeholder="Question">
  <input type="text" name="question" placeholder="Answers">
  <input type="text" name="corect" placeholder="Correct Answers">
